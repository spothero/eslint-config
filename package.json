{
  "name": "@spothero/eslint-config",
  "version": "1.9.1",
  "description": "SpotHero's base ESLint configuration.",
  "main": "index.js",
  "repository": "https://github.com/spothero/eslint-config",
  "author": "SpotHero",
  "license": "Apache-2.0",
  "eslintConfig": {
    "extends": [
      "./index.js",
      "plugin:prettier/recommended",
      "prettier/babel",
      "prettier/react"
    ]
  },
  "prettier": "@spothero/prettier-config",
  "scripts": {
    "release": "npm-release -- public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1"
  },
  "devDependencies": {
    "@spothero/commitlint-config": "2.1.2",
    "@spothero/prettier-config": "0.1.1",
    "@spothero/npm-publisher": "2.3.2"
  },
  "peerDependencies": {
    "@spothero/prettier-config": "^0.1.1"
  },
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=6.1.0"
  }
}
