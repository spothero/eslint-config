{
  "name": "@spothero/eslint-config",
  "version": "2.1.0",
  "description": "SpotHero's base ESLint configuration.",
  "main": "index.js",
  "repository": "https://github.com/spothero/eslint-config",
  "author": "SpotHero",
  "license": "Apache-2.0",
  "babel": {
    "presets": [
      "@spothero/spothero"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./index.js",
      "plugin:prettier/recommended",
      "prettier/babel",
      "prettier/react"
    ]
  },
  "prettier": "@spothero/prettier-config",
  "scripts": {
    "release": "npm-release -- public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@babel/core": "7.11.1",
    "@babel/eslint-parser": "7.11.3",
    "@babel/eslint-plugin": "7.11.3",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0"
  },
  "devDependencies": {
    "@spothero/babel-preset-spothero": "1.1.0",
    "@spothero/commitlint-config": "2.3.0",
    "@spothero/npm-publisher": "2.4.2",
    "@spothero/prettier-config": "0.3.0",
    "react-hot-loader": "4.12.21"
  },
  "peerDependencies": {
    "@spothero/prettier-config": "^0.2.0"
  },
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.12.0"
  }
}
